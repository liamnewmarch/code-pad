{
  "name": "fiddle",
  "version": "3.1.0",
  "description": "A HTML / CSS / JS sandbox that works offline and between page refreshes.",
  "private": true,
  "homepage": "https://github.com/liamnewmarch/fiddle#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamnewmarch/fiddle.git"
  },
  "bugs": {
    "url": "https://github.com/liamnewmarch/fiddle/issues"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c rollup.config.js",
    "serve": "firebase serve",
    "start": "concurrently npm:watch npm:serve --kill-others",
    "watch": "NODE_ENV=development rollup -w -c rollup.config.js"
  },
  "babel": {
    "plugins": [
      "external-helpers",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ]
  },
  "browserlist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "codemirror": "^5.39.2",
    "codemirror-one-dark-theme": "^1.1.1",
    "concurrently": "^3.6.1",
    "eslint": "^5.3.0",
    "firebase": "^5.4.0",
    "firebase-tools": "^4.0.3",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1"
  }
}
