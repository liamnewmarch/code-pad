{
  "name": "fiddle",
  "version": "3.1.3",
  "description": "A HTML / CSS / JS sandbox that works offline and between page refreshes.",
  "private": true,
  "homepage": "https://github.com/liamnewmarch/fiddle#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamnewmarch/fiddle.git"
  },
  "bugs": {
    "url": "https://github.com/liamnewmarch/fiddle/issues"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c rollup.config.js",
    "deploy": "npm run build && firebase deploy",
    "serve": "firebase serve",
    "start": "concurrently npm:watch npm:serve --kill-others",
    "watch": "NODE_ENV=development rollup -w -c rollup.config.js"
  },
  "babel": {
    "plugins": [
      "external-helpers",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ]
  },
  "browserlist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^5.0.0",
    "firebase": "^5.7.2",
    "firebase-tools": "^6.2.2",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "vue-template-compiler": "^2.5.21"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "codemirror": "^5.42.2",
    "codemirror-one-dark-theme": "^1.1.1",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  }
}
