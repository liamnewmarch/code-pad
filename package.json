{
  "name": "fiddle",
  "version": "3.2.0",
  "description": "A HTML / CSS / JS sandbox that works offline and between page refreshes.",
  "private": true,
  "homepage": "https://github.com/liamnewmarch/fiddle#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamnewmarch/fiddle.git"
  },
  "bugs": {
    "url": "https://github.com/liamnewmarch/fiddle/issues"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c rollup.config.js",
    "deploy": "npm run build && firebase deploy",
    "serve": "firebase serve",
    "start": "concurrently npm:watch npm:serve --kill-others",
    "watch": "NODE_ENV=development rollup -w -c rollup.config.js"
  },
  "dependencies": {
    "codemirror": "^5.45.0",
    "codemirror-one-dark-theme": "^1.1.1",
    "rollup": "^1.7.0",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.10",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "firebase-tools": "^6.5.0"
  }
}
